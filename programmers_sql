#보호소에서 중성화한 동물
SELECT AO.ANIMAL_ID, AO.ANIMAL_TYPE,AO.NAME
FROM ANIMAL_OUTS AO
LEFT JOIN ANIMAL_INS AI ON AI.ANIMAL_ID = AO.ANIMAL_ID
WHERE AI.SEX_UPON_INTAKE like "Intact%" AND (AO.SEX_UPON_OUTCOME like "Spayed%" or AO.SEX_UPON_OUTCOME like "Neutered%")
ORDER BY AO.ANIMAL_ID ASC;

#오랜 기간 보호한 동물(1)
SELECT AI.NAME,AI.DATETIME
FROM ANIMAL_INS AI
LEFT JOIN ANIMAL_OUTS AO ON AO.ANIMAL_ID = AI.ANIMAL_ID
WHERE AI.ANIMAL_ID NOT IN (SELECT AO2.ANIMAL_ID FROM ANIMAL_OUTS AO2)
ORDER BY AI.DATETIME ASC
LIMIT 3;

#있었는데요 없었습니다

SELECT AO.ANIMAL_ID,AO.NAME
FROM ANIMAL_OUTS AO
LEFT JOIN ANIMAL_INS AI ON AI.ANIMAL_ID = AO.ANIMAL_ID
WHERE AI.DATETIME> AO.DATETIME
ORDER BY AI.DATETIME ASC;

#없어진 기록 찾기

SELECT AO.ANIMAL_ID, AO.NAME
FROM ANIMAL_OUTS AO
LEFT JOIN ANIMAL_INS AI ON AI.ANIMAL_ID = AO.ANIMAL_ID
WHERE AI.DATETIME IS NULL
ORDER BY AO.ANIMAL_ID ASC;
